<h1>Listing users <%= params[:search_string] %></h1>

<table>
  <tr>
    <th>Empid</th>
    <th>Name</th>
    <th>Age</th>
    <th>Phone no</th>
    <th>Email Id</th>
    <th>Address</th>
    <th>Department</th>
    <th>Experience</th>
    <th>Qualification</th>
    <th>Location</th>
    <th>Role</th>
  </tr>
<% for user in @users %>
  <tr>
    <td><%=h user.empid %></td>
    <td><%=h user.name %></td>
    <td><%=h user.age %></td>
    <td><%=h user.phone_no %></td>
    <td><%=h user.email_id %></td>
    <td><%=h user.address %></td>
    <td><%=h user.department %></td>
    <td><%=h user.experience %></td> 
    <td><%=h user.qualification %></td>
    <td><%=h user.location %></td>
    <td><%=h user.role %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
 </table>								